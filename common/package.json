{
    "name": "@local/common",
    "version": "2.0.0",
    "description": "",
    "author": "Amelia Bleeker (PNNL)",
    "private": true,
    "license": "SEE LICENSE IN ../LICENSE.txt",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc --project tsconfig.build.json",
        "lint": "eslint \"src/**/*.ts\" --fix",
        "check": "tsc --noEmit",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage"
    },
    "dependencies": {
        "@local/prisma": "file:../prisma",
        "@prisma/client": "^6.9.0",
        "lodash": "^4.17.21",
        "regex": "^6.0.1"
    },
    "devDependencies": {
        "@eslint/eslintrc": "^3.2.0",
        "@eslint/js": "^9.18.0",
        "@pothos/core": "^4.3.0",
        "@pothos/plugin-prisma": "^4.4.0",
        "@pothos/plugin-prisma-utils": "^1.2.0",
        "@types/jest": "^29.5.14",
        "@types/lodash": "^4.17.15",
        "@types/node": "^22.10.7",
        "eslint": "^9.18.0",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^15.14.0",
        "graphql": "^16.10.0",
        "jest": "^29.7.0",
        "prettier": "^3.4.2",
        "prisma": "^6.9.0",
        "prisma-json-types-generator": "^3.2.3",
        "ts-jest": "^29.2.5",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.7.3",
        "typescript-eslint": "^8.20.0",
        "webpack": "^5.99.5"
    },
    "peerDependencies": {
        "@local/prisma": "*",
        "@prisma/client": "^6.6.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "moduleNameMapper": {
            "@/(.*)": "<rootDir>/$1"
        },
        "testRegex": ".*\\.(?:spec|test)\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "coverageReporters": [
            "json",
            "text",
            "lcov",
            "clover",
            "cobertura"
        ],
        "testEnvironment": "node"
    },
    "packageManager": "yarn@4.9.1"
}
