{
  "name": "@local/prisma",
  "version": "2.0.3",
  "description": "",
  "author": "Amelia Bleeker (PNNL)",
  "private": true,
  "license": "SEE LICENSE IN ../LICENSE.txt",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npx prisma generate && tsc --project tsconfig.build.json",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "check": "tsc --noEmit",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --passWithNoTests --watch",
    "test:cov": "jest --passWithNoTests --coverage",
    "prisma": "npx prisma",
    "migrate": "yarn migrate:deploy",
    "migrate:create": "yarn prisma migrate dev",
    "migrate:deploy": "yarn prisma migrate deploy",
    "migrate:reset": "yarn prisma migrate reset",
    "postinstall": "npx prisma generate"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@pothos/core": "^4.3.0",
    "@pothos/plugin-prisma": "^4.4.0",
    "@pothos/plugin-prisma-utils": "^1.2.0",
    "@prisma/client": "^6.9.0",
    "@types/geojson": "^7946.0.16",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-formatter-gitlab": "^6.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "graphql": "^16.10.0",
    "jest": "^29.7.0",
    "prisma": "^6.9.0",
    "prisma-json-types-generator": "^3.2.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "peerDependencies": {
    "@prisma/client": "^6.6.0"
  },
  "prisma": {
    "schema": "prisma"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "moduleNameMapper": {
      "@/(.*)": "<rootDir>/$1"
    },
    "testRegex": ".*\\.(?:spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "json",
      "text",
      "lcov",
      "clover",
      "cobertura"
    ],
    "testEnvironment": "node"
  },
  "packageManager": "yarn@4.9.1"
}
